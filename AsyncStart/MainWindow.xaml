<Window x:Class="AsyncStart.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncStart"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="TextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Name="Button" Content="Button" HorizontalAlignment="Left" Margin="231,18,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Name="ButtonAcync" Content="ButtonAcync" HorizontalAlignment="Left" Margin="231,43,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAcync_Click"/>
        <Button Name="StartButton" Content="StartButton" HorizontalAlignment="Left" Margin="231,68,0,0" VerticalAlignment="Top" Width="75" Click="StartButton_Click"/>
        <Label Name="StatusLabel" Content="Label" HorizontalAlignment="Left" Margin="10,131,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
